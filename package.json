{
  "name": "boiler-app",
  "version": "1.0.0",
  "main": "index.ts",
  "scripts": {
    "start": "expo start",
    "start:test": "node scripts/set-env.js test && expo start",
    "start:dev": "node scripts/set-env.js development && expo start",
    "start:prod": "node scripts/set-env.js production && expo start",
    "env:test": "node scripts/set-env.js test",
    "env:dev": "node scripts/set-env.js development",
    "env:prod": "node scripts/set-env.js production",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "test": "jest",
    "lint": "eslint src --ext .ts,.tsx --fix",
    "lint:check": "eslint src --ext .ts,.tsx",
    "prettier": "prettier --write \"src/**/*.{ts,tsx,json}\"",
    "prettier:check": "prettier --check \"src/**/*.{ts,tsx,json}\"",
    "typecheck": "tsc --noEmit",
    "build": "npm run typecheck",
    "build:android": "eas build --platform android",
    "build:ios": "eas build --platform ios",
    "storybook": "node scripts/storybook-toggle.js storybook && npm start",
    "storybook:app": "node scripts/storybook-toggle.js app && npm start",
    "storybook:toggle": "node scripts/storybook-toggle.js",
    "storybook:generate": "sb init --type react_native",
    "storybook:build": "storybook build"
  },
  "dependencies": {
    "@expo/vector-icons": "^14.1.0",
    "@hookform/resolvers": "^5.2.0",
    "@react-native-async-storage/async-storage": "2.1.2",
    "@react-native-community/datetimepicker": "8.4.1",
    "@react-navigation/drawer": "^7.3.9",
    "@react-navigation/native": "^7.1.6",
    "@react-navigation/native-stack": "^7.3.10",
    "@tanstack/react-query": "^5.83.0",
    "axios": "^1.11.0",
    "date-fns": "^4.1.0",
    "eas-cli": "^16.17.4",
    "expo": "~53.0.20",
    "expo-image-picker": "^16.1.4",
    "expo-linking": "~7.1.7",
    "expo-location": "~18.1.6",
    "expo-navigation-bar": "~4.2.7",
    "expo-notifications": "~0.31.4",
    "expo-router": "^5.1.4",
    "expo-status-bar": "~2.2.3",
    "lottie-react-native": "7.2.2",
    "react": "19.0.0",
    "react-hook-form": "^7.61.1",
    "react-native": "0.79.5",
    "react-native-gesture-handler": "~2.24.0",
    "react-native-get-random-values": "^1.11.0",
    "react-native-google-places-autocomplete": "^2.5.7",
    "react-native-maps": "1.20.1",
    "react-native-maps-directions": "^1.9.0",
    "react-native-reanimated": "~3.17.4",
    "react-native-safe-area-context": "5.4.0",
    "react-native-screens": "~4.11.1",
    "react-native-toast-message": "^2.3.3",
    "react-native-worklets-core": "^1.6.0",
    "socket.io-client": "^4.8.1",
    "styled-components": "^6.1.19",
    "zod": "^4.0.10",
    "zustand": "^5.0.6"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@expo/metro-config": "^0.20.17",
    "@storybook/addon-ondevice-actions": "^9.1.0",
    "@storybook/addon-ondevice-controls": "^9.1.0",
    "@storybook/addon-ondevice-knobs": "^7.6.20",
    "@storybook/react-native": "^9.1.0",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^13.2.0",
    "@types/jest": "^30.0.0",
    "@types/react": "~19.0.10",
    "@typescript-eslint/eslint-plugin": "^8.19.1",
    "@typescript-eslint/parser": "^8.19.1",
    "babel-jest": "^30.0.5",
    "babel-plugin-module-resolver": "^5.0.2",
    "eslint": "^9.16.0",
    "jest": "~29.7.0",
    "jest-expo": "^53.0.9",
    "metro-react-native-babel-preset": "^0.77.0",
    "prettier": "^3.4.2",
    "typescript": "~5.8.3"
  },
  "jest": {
    "preset": "jest-expo",
    "setupFilesAfterEnv": [
      "@testing-library/jest-native/extend-expect"
    ]
  },
  "private": true
